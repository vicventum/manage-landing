{
  "name": "Manage",
  "version": "1.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "parcel src/index.html src/post.html src/signin.html",
    "save-dev": "npm install --save-dev @babel/core @babel/cli @babel/preset-env @babel/plugin-transform-runtime sass postcss-modules autoprefixer ",
    "save": "npm install --save @babel/runtime core-js include-media",
    "build": "parcel build ./src/index.html --out-dir docs",
    "deploy": "firebase deploy"
  },
  "keywords": [
    "javascript",
    "sass",
    "game"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          ">1%",
          "last 4 versions",
          "Firefox ESR",
          "ie > 9"
        ]
      },
      "rfs": {}
    }
  },
  "author": "Victor Alvarez <vicventum.dev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "core-js": "^3.6.5",
    "include-media": "^1.4.9",
    "src": "^1.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "autoprefixer": "^9.8.6",
    "cssnano": "^4.1.10",
    "postcss-modules": "^3.2.2",
    "rfs": "^9.0.3",
    "sass": "^1.26.10"
  }
}
